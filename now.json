{
  "name": "myvideo",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@myvideo_db_user",
    "DB_PASSWORD": "@myvideo_db_password",
    "DB_HOST": "@myvideo_db_host",
    "DB_NAME": "@myvideo_db_user",
    "AUTH_JWT_SECRET": "@myvideo-auth-jwt-secret",
    "PUBLIC_API_KEY": "@myvideo-public-api-key",
    "ADMIN_API_KEY": "@myvideo-admin-api-key",
    "DEFAULT_ADMIN_PASSWORD": "@myvideo-default-admin-password",
    "DEFAULT_USER_PASSWORD": "@myvideo-default-user-password",
    "PORT": "@myvideo-port"
  }
}
